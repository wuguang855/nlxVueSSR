{
  "name": "vue-ssr",
  "description": "A vue-ssr project",
  "author": "wuguang",
  "private": true,
  "scripts": {
    "dev": "cross-env PORT=9099 NODE_ENV=development node teamall",
    "start": "cross-env NODE_ENV=production node server",
